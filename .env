# FeedJam - Base Configuration (safe to commit)
# Local overrides go in .env.local (gitignored)
# Production values go in .env.prod (gitignored, on server only)

# Database
POSTGRES_USER=feedjam
POSTGRES_PASSWORD=feedjam
POSTGRES_DB=feedjam
POSTGRES_PORT=5433

# Redis
REDIS_PORT=6380

# API
API_PORT=8001
DEBUG_PORT=5679

# Auth (override in .env.local with real key)
JWT_SECRET_KEY=dev-secret-key-change-in-production

# Features
ENABLE_SUMMARIZATION=true
CREATE_ITEMS_ON_STARTUP=false

# OpenAI (optional, set in .env.local)
OPEN_AI_KEY=

# Docker
DOCKERHUB_USERNAME=feedjam
